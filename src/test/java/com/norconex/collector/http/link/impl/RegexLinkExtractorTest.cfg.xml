<!DOCTYPE xml>
<extractor class="com.norconex.collector.http.link.impl.RegexLinkExtractor">
  <linkExtractionPatterns>
    <pattern>
      <match><![CDATA[<a href="javascript:;"[^>]*?id="p_(\d+)">]]></match>
      <replace>/page?id=$1</replace>
    </pattern>
  </linkExtractionPatterns>
</extractor>